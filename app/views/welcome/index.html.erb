<form id="sign-up-form">
<label>Name:</label>
<input id="signup-name">
<label>Email:</label>
<input id="signup-email" type="email" />
<label>Password:</label>
<input id="signup-password" type="password" />
<label>Password Confirmation:</label>
<input id="signup-password-confirmation" type="password" />
<button id="sign-up" class="submit">Sign up</button>
</form>

<form id="log-in-form">
<label>Email:</label>
<input id="login-email" type="email" />
<label>Password:</label>
<input id="login-password" type="password" />
<button id="log-in" class="submit">Log in</button>
</form>

<button id='log-out' class="submit">Log Out</button>

<h1>Let's Get Weird</h1>

<button id="start-pause-timer">Start Timer</button>
<button id="reset-timer">Reset Timer</button>

<script>
  window.soundURLs = <%= raw(@sounds.to_json) %>
</script>

<form id="tempo">
  <input name="BPM"
         type="number"
         min="20"
         max="240"
         step="1"
         value="120">
  <input type="submit" value="Set BPM">
</form>

<%= @buckets %>
<br>

<div class="sequencer-wrapper">

  <div class="suite-wrapper">
    <div class="column" id="drop-column">
  </div>

</div>

<form>
  <label>Song Name</label>
  <input id="song-title"></input>
  <input type='submit' name='save' id='save'></input>
</form>

